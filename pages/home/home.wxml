<scroll-view scroll-y class="scrollPage backg" 
bindscrolltoupper="upper" 
bindscrolltolower="lower" 
bindscroll="scroll"
enable-back-to-top="true"
scroll-with-animation
 >
  
    <!-- <cu-custom bgColor="bg-gradual-blue" isCustom="true"> -->
     <view class="cu-bar bg-cyan search" style='height: 150rpx'>
      <view class="search-form radius">
        <text class="cuIcon-search"></text>
        <input type="text" placeholder="搜索车辆" confirm-type="search"></input>
      </view>
      <view class="action">
        <text>{{address.city}}</text>
        <text class="cuIcon-triangledownfill"></text>
      </view>
    </view>
   <!-- </cu-custom>  -->
   


  
  <view class='nav-list home-container'>
    <map id="map" longitude="{{position.longitude}}" latitude="{{position.latitude}}" scale="18" controls="{{controls}}" bindcontroltap="controltap" markers="{{markers}}" bindmarkertap="markertap" polyline="{{polyline}}" bindregionchange="regionchange" show-location style="width: 100%; height: 100%;"></map>
  </view>
  
  <view class='cu-tabbar-height'></view>
</scroll-view>
